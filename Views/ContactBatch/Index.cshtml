@using UploadExcelFile.Models
@model List<ContactBatch>


@{ ViewBag.Title = "File Batch";

       Layout = "~/Views/Shared/_Layout.cshtml"; 
 }

<h2>File Batch</h2>
@if (Model.Count() > 0)
{
<hr />
<table class="table table-bordered">
    <tr>
        <td>Batch Id</td>
        <td>Batch Name</td>
        <td>Created By</td>
        <td>Date Created</td>
        <td>Action</td>

    </tr>

    @foreach (ContactBatch contact in Model)
    {
<tr>
    <td>@contact.BatchID</td>
    <td>@contact.BatchName</td>
    <td>@contact.CreatedBy</td>
    <td>@contact.DateCreated</td>
    <td>
        <button class="btn btn-default">
            <span style="color:white">@Html.ActionLink("View", "GetFileById", "ContactBatch", new { id = contact.BatchID }, null)</span>
        </button> | <button class="btn btn-default">
            <span style="color:white">@Html.ActionLink("Edit", "EditById", "ContactBatch", new { id = contact.BatchID }, null)</span>
        </button> | <button class="btn btn-default">
            <span style="color:white">@Html.ActionLink("Delete", "DeleteById", "ContactBatch", new { id = contact.BatchID }, null)</span>
        </button>
    </td>
</tr>
    }
</table>
}

<button class="btn btn-default">
    <span style="color:white">@Html.ActionLink("Go To Validate", "Index", "Contacts")</span>
</button>